    "@types/node": "^22.15.18",
    "@types/sequelize": "^4.28.20",
    "@types/styled-components": "^5.1.34",
    "sequelize-cli": "^6.6.3",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
